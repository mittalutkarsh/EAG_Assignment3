{
  "manifest_version": 3,
  "name": "Finance Agent Extension",
  "version": "1.0",
  "description": "Create and evaluate trading strategies using Gemini AI",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ]
} 